{
  "name": "uniapp-sample",
  "version": "1.0.0",
  "scripts": {
    "tailwind": "npx tailwindcss -i ./index.css -o ./tailwind.css --watch",
    "tailwind.uniapp": "node ./tailwind.help.js",
    "update-dep": "yarn upgrade-interactive --latest",
    "up2lib": "rm -r ../../uniapp-sample/lib/* && cp -r lib/* ../../uniapp-sample/lib/",
    "up4lib": "rm -r lib/* && cp -r ../../uniapp-sample/lib/* lib/"
  },
  "devDependencies": {
    "node-watch": "^0.7.3",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
